  set -l yellow (set_color -o yellow)
  set -l red (set_color -o red)
  set -l blue (set_color -o blue)
  set -l green (set_color -o green)
  set -l normal (set_color normal)
  # set_color cyan; echo "ʕっ•ᴥ•ʔっ hello"
end
